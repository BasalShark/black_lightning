.no-wrap {
    white-space: nowrap
}


/* Primarily for use in markdown  */
.captioned-image {
  max-width: 192px;
  font-size: smaller;
}

.captioned-image p:last-child {
  margin: 0px;
}

img.float-left, img.float-right {
  @extend .img-fluid !optional;
  max-width: 192px;
}

.center {
  text-align: center;
}
