<div class="card">
  <div class="card-header"><%= @description %></div>
  <div class="card-body">
    <div class="accordion" id="jobsEnqueued">
      <% delayed_jobs(@type).each_with_index do |job, index| %>
        <%= render 'job', job: job, index: index %>
      <% end %>
    </div>
  </div>
</div>
