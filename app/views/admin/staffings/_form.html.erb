<%= simple_form_for(@admin_staffing) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
     
      <%= f.input :date%>
     
      <%= f.input :show_title %>
     
      <div class="jobs">
        <%= f.simple_fields_for :staffing_jobs do |j| %>
          <%= j.input :name %>
            
          <%= j.input :user_id, :collection => User.all.collect {|u| ["#{u.first_name} #{u.last_name}".presence || u.email, u.id]} %>
        <% end %>
      </div>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to 'Cancel', admin_staffings_path, :class => "btn" %>
  </div>
<% end %>
