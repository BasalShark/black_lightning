<h1>Listing all users</h1>
<table class="table">
	<tbody>
		<tr>
			<th>Email</th>
			<th>Name</th>
			<th></th>
		</tr>
		<% @users.each do |user| %>
		<tr>
			<td><%= user.email %></td>
			<td><%= "#{user.first_name} #{user.last_name}" %></td>
			<td><%= link_to 'Edit', edit_admin_user_path(user) %></td>
		</tr>
		<% end %>
	</tbody>
</table>
<%= link_to 'New user', new_admin_user_path, :class => "btn" %>