<% headers = [:name, :tagline] %>

<% field_sets = @venues.map { |venue| { 
  fields: [
  venue,  
  venue.tagline
] } } %>

<% search_fields = {
    name_cont:        { slug: 'defaults.name' },
    tagline_cont:     { slug: 'defaults.tagline' },
    description_cont: { slug: 'defaults.description'}
} %>

<%= render 'shared/pages/index', resource_class: Venue, resources: @venues, headers: headers, field_sets: field_sets, search_fields: search_fields %>
