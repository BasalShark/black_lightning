<% if cookies['allow_cookies'] %>
<script type="text/javascript">

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-18356204-1']);
  _gaq.push(['_setSiteSpeedSampleRate', 5]);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

  $(document).ready(function() {
    doc_ready = new Date().getTime();

    var timeSpent = doc_ready - load_start;
    var hour = 1000 * 60 * 60;

    if (0 < timeSpent && timeSpent < hour) {
      _gaq.push(['_trackTiming', 'pageLoad', 'Document Ready', timeSpent]);
    }
  });

  $(window).on('load', function() {
    load_end = new Date().getTime();

    var timeSpent = doc_ready - load_start;
    var hour = 1000 * 60 * 60;

    if (0 < timeSpent && timeSpent < hour) {
      _gaq.push(['_trackTiming', 'pageLoad', 'Window Loaded', timeSpent]);
    }
  });

</script>
<% end %>
