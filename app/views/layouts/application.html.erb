<!DOCTYPE html>
<html>
<head>
  <title>Bedlam Theatre<%= " | #{@title}" unless @title == nil%></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  
  <%= yield :head %>
</head>
<body>
  <div class="container">
  	<!-- START HEADER SECTION -->
	<div class="row header">
			
		<div class="row">
			<div class="span4">
				<%= image_tag 'BedlamLogoBW.png', :id => "logo"%>
			</div>
			<div class="span8">
				<div class="row">
					<div class="row pull-right">
						<a href="#" class="btn">Press</a>
						<a href="#" class="btn">Sponsors</a>
						<a href="#" class="btn">Members</a>
						<a href="#" class="btn">Newsletter</a>
					</div>
					<div class="row">
						<ul id="nav">
							<li><a href="#">Shows</a></li>
							<li><a href="#">News</a></li>
							<li><a href="#">About</a></li>
							<li><a href="#">Get Involved</a></li>
							<li><a href="#">Contact</a></li>
						</ul>
					</div>
				</div>
			</div>
		</div>

	</div>
<%= render :partial => "shared/cookie_notice" %>

<%= yield %>

<%= render :partial => "shared/google_analytics" %>
</div>
</body>
</html>
