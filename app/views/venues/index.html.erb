<% search_form = nil %>

<% items = @venues.collect { |venue| { 
  url: url_for(venue),
  image: venue.fetch_image,
  header: venue.name,
  paragraphs: [
    { content: venue.tagline }
  ],
  button_text: 'View Details'
  } }
%>

<%= render 'shared/pages/public/index', search_form: search_form, items: items, resources: @venues %>
